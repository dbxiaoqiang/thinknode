{
  "name": "thinknode",
  "version": "2.4.6",
  "description": "Hello ThinkNode! A Node.js MVC Framework use full ES6/7 features.",
  "scripts": {
    "test": "npm run eslint && npm run test-cov",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 10000 --recursive  -R spec test/",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir lib/",
    "watch-compile": "npm run compile -- --watch",
    "prepublish": "npm run compile",
    "eslint": "eslint src/"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richenlin/thinknode.git"
  },
  "keywords": [
    "think",
    "thinknode",
    "framework",
    "web",
    "mvc",
    "es6/es7",
    "restful"
  ],
  "engines": {
    "node": ">4.0.0"
  },
  "author": {
    "name": "richenlin",
    "email": "richenlin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richenlin/thinknode/issues"
  },
  "homepage": "https://github.com/richenlin/thinknode",
  "devDependencies": {
    "mocha": "1.20.1",
    "muk": "0.3.1",
    "istanbul": "0.4.0",
    "eslint": "1.10.2",
    "babel-cli": "6.3.17",
    "babel-preset-es2015-loose": "6.1.4",
    "babel-preset-stage-1": "6.3.13",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-eslint": "*",
    "babel-core": "6.x.x"
  },
  "dependencies": {
    "bluebird": "3.1.1",
    "ejs": "2.3.4",
