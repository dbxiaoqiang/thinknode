{
  "name": "thinknode-application",
  "description": "application created by thinknode",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/index.js",
    "compile": "babel --loose all --optional runtime --stage 0 --copy-files --modules common src/ --out-dir App/ --retain-lines",
    "watch-compile": "npm run compile -- --watch",
    "watch": "npm run watch-compile"
  },
  "dependencies": {
    "thinknode": ">=2.1.0",
    "babel-runtime": "5.8.29",
    "babel-core": "5.8.33"
  }
}